{
  "name": "barkbaud-ui",
  "version": "1.0.0",
  "scripts": {
    "typings-install": "typings install",
    "typings": "typings",
    "postinstall": "npm run typings-install",
    "clean": "npm cache clean && npm run rimraf -- node_modules typings build",
    "build": "grunt build; webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000 --content-base src",
    "start": "npm run server"
  },
  "devDependencies": {
    "awesome-typescript-loader": "0.17.0",
    "raw-loader": "0.5.1",
    "sass-loader": "3.2.0",
    "json-loader": "0.5.4",
    "node-sass": "3.5.3",
    "es6-promise": "3.0.2",
    "es6-shim": "0.35.0",
    "grunt": "^0.4.5",
    "grunt-bump": "~0.3.0",
    "grunt-concat-sourcemap": "~0.4.3",
    "grunt-contrib-copy": "~0.7.0",
    "grunt-contrib-sass": "~0.8.1",
    "grunt-contrib-uglify": "~0.7.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-html2js": "^0.3.2",
    "grunt-skylint": "git+https://github.com/blackbaud/grunt-skylint",
    "es7-reflect-metadata": "1.6.0",
    "source-map-loader": "0.1.5",
    "typescript": "1.8.10",
    "typings": "0.8.1",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.12.0",
    "ts-helpers": "1.1.1"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "blackbaud-skyux2": "git+http://github.com/blackbaud/skyux2#master",
    "core-js": "2.4.0",
    "rxjs": "5.0.0-beta.6",
    "symbol-observable": "0.2.4",
    "zone.js": "0.6.12"
  }
